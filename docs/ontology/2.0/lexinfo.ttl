lemon:Phrase
    a owl:Class .

lemon:PropertyValue
    a owl:Class .

lemon:RaisableArgument
    a owl:Class ;
    rdfs:subClassOf lemon:Argument .

lemon:SenseContext
    a owl:Class .

lemon:Word
    a owl:Class .

lemon:broader
    a owl:ObjectProperty .

lemon:context
    a owl:ObjectProperty .

lemon:definition
    a owl:ObjectProperty .

lemon:equivalent
    a owl:ObjectProperty .

lemon:formVariant
    a owl:ObjectProperty .

lemon:incompatible
    a owl:ObjectProperty .

lemon:lexicalVariant
    a owl:ObjectProperty .

lemon:marker
    a owl:ObjectProperty .

lemon:narrower
    a owl:ObjectProperty .

lemon:optional
    a owl:DatatypeProperty .

lemon:property
    a owl:ObjectProperty .

lemon:representation
    a owl:DatatypeProperty .

lemon:senseRelation
    a owl:ObjectProperty .

lemon:separator
    dcr:datcat <http://www.isocat.org/datcat/DC-1863> ;
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> .

lemon:synArg
    a owl:ObjectProperty .

lemon:synBehavior
    a owl:ObjectProperty .

