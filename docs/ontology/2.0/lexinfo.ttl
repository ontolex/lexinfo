<http://www.lexinfo.net/ontology/2.0/lexinfo>
    ns:license <https://creativecommons.org/licenses/by/4.0/> ;
    terms:contributor <http://john.mccr.ae/>, <http://www.cimiano.de/philipp>, <http://www.paulbuitelaar.net/> ;
    terms:creator <http://john.mccr.ae/> ;
    terms:description "An ontology of types, values and properties to be used with the lemon model, partially derived from ISOcat."@en ;
    terms:issued "2010-11-22"^^xsd:date ;
    terms:modified "2014-06-14"^^xsd:date ;
    terms:publisher <http://john.mccr.ae/> ;
    terms:rights <https://creativecommons.org/licenses/by/4.0/>, "Copyright 2010 Monnet Project"@en ;
    terms:title "LexInfo"@en ;
    vann:preferredNamespacePrefix "lexinfo" ;
    vann:preferredNamespaceUri "http://www.lexinfo.net/ontology/2.0/lexinfo#"^^xsd:anyURI ;
    a <http://purl.org/vocommons/voaf#Vocabulary>, owl:Ontology ;
    rdfs:comment "An ontology of types, values and properties to be used with the lemon model, partially derived from ISOcat."@en ;
    rdfs:label "LexInfo"@en ;
    owl:imports <http://lemon-model.net/lemon> ;
    owl:versionInfo "2.0"@en .

:AbbreviatedForm
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :TermType .

:AccusativePostPositiveArgument
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PostPositiveArgument ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:PostPositiveArgument
            [
                a owl:Restriction ;
                owl:hasValue :accusativeCase ;
                owl:onProperty lemon:marker
            ]
        )
    ] .

:AdjectivalComplementFrame
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :predicativeAdjective
            ]
        )
    ] .

:Adjective
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Word, [
        a owl:Restriction ;
        owl:allValuesFrom :AdjectiveFrame ;
        owl:onProperty lemon:synBehavior
    ] ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :AdjectivePOS
    ] .

:AdjectiveAccusativePostPositiveFrame
    :example "wird wurden unseren irrtum GEWAHR"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :AccusativePostPositiveArgument
            ]
        )
    ] .

:AdjectiveAttributiveFrame
    :example "the RED ball"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :attributiveArg
            ]
        )
    ] .

:AdjectiveComparativeFrame
    :example "new york is BIGGER THAN berlin"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :comparativeAdjunct
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] .

:AdjectiveDativePostPositiveFrame
    :example "er ist seinem bruder Ã„HNLICH"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :DativePostPositiveArgument
            ]
        )
    ] .

:AdjectiveFrame
    a owl:Class ;
    rdfs:subClassOf lemon:Frame .

:AdjectiveGenitivePostPositiveFrame
    :example "er ist des hochverrats SCHULDIG"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :GenitivePostPositiveArgument
            ]
        )
    ] .

:AdjectiveImpersonalFrame
    :example "es ist mir HEISS"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :object
            ]
            [
                a owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] .

:AdjectivePOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PartOfSpeech .

:AdjectivePPFrame
    :example "he is RELATED TO her"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePredicativeFrame
            :PrepositionalFrame
        )
    ] .

:AdjectivePhrase
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Phrase ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (lemon:Phrase
            [
                a owl:Restriction ;
                owl:onProperty :head ;
                owl:someValuesFrom :Adjective
            ]
        )
    ] .

:AdjectivePostPositiveFrame
    :example "that is something INTERESTING"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeArg
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :postPositiveArg
            ]
        )
    ] .

:AdjectivePredicateFrame
    a owl:Class ;
    rdfs:comment "Note this frame should be used when both attributive and predicative use of the adjective is allowed"@en ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveAttributiveFrame
            :AdjectivePredicativeFrame
        )
    ] .

:AdjectivePredicativeFrame
    :example "he is HAPPY"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] .

:AdjectiveScaleFrame
    :example "Everest is 8,848m HIGH"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf ([
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :adverbialComplement
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] .

:AdjectiveSuperlativeFrame
    :example "tokyo is the BIGGEST of all metropoles"@en ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :superlativeAdjunct
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] .

:Adjunct
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Argument, [
        a owl:Restriction ;
        owl:hasValue true ;
        owl:onProperty lemon:optional
    ] .

:Adposition
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Word ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :AdpositionPOS
    ] .

:AdpositionPOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PartOfSpeech .

:AdpositionalObject
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Object, [
        a owl:Restriction ;
        owl:onProperty lemon:marker ;
        owl:someValuesFrom :Adposition
    ] ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:Object
            [
                a owl:Restriction ;
                owl:onProperty lemon:marker ;
                owl:someValuesFrom :Adposition
            ]
        )
    ] .

:Adverb
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Word ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :AdverbPOS
    ] .

:AdverbPOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PartOfSpeech .

:AdverbialComplementFrame
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :predicativeAdverb
            ]
        )
    ] .

:Affix
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Part ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:hasValue :affix ;
        owl:onProperty :termElement
    ] .

:Animacy
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:ArbitraryControl
    a owl:Class ;
    rdfs:comment "Indicates either the subject or object of the main clause may be the omitted argument of the subclause"@en ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Control .

:Article
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Determiner ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :ArticlePOS
    ] .

:ArticlePOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :DeterminerPOS .

:Aspect
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:AttributiveArgument
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Argument .

:CJK_compound
    dc:creator "Wright, Sue Ellen" ;
    dcr:datcat <http://www.isocat.org/datcat/DC-356> ;
    a :TermType, owl:NamedIndividual, owl:Thing ;
    rdfs:comment "A lexical unit in a CJKV language that is represented by at least two CJKV characters." ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:label "compound(cjkv)" ;
    owl:versionInfo "1:0" .

:Case
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:ClausalArgument
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Argument .

:Cliticness
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:CommonNoun
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Noun ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:hasValue :commonNoun ;
        owl:onProperty :partOfSpeech
    ] .

:ComparativeAdjunct
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Adjunct .

:Complement
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Argument .

:Conjunction
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Word ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :ConjunctionPOS
    ] .

:ConjunctionPOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PartOfSpeech .

:Control
    a owl:Class ;
    rdfs:comment "Control indicates how a an argument from a main clause will be utilized in a subclause. This class includes both control structures and raising structures"@en ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Frame .

:Dating
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:PropertyValue .

:DativePostPositiveArgument
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PostPositiveArgument ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:PostPositiveArgument
            [
                a owl:Restriction ;
                owl:hasValue :dativeCase ;
                owl:onProperty lemon:marker
            ]
        )
    ] .

:DativeTransitiveFrame
    :example "ich DANKE dir"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty :directObject
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :indirectObject
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :subject
            ]
        )
    ] .

:DeclarativeClause
    a owl:Class ;
    rdfs:comment "A clause beginning with the declarative conjunction (\"that\" in English)"@en ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :ClausalArgument .

:DeclarativeFrame
    a owl:Class ;
    rdfs:comment "Declarative frames take a clause as an argument, this argument is marker with the declarative conjunction (\"that\" in English)"@en ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :declarativeClause
            ]
        )
    ] .

:Definiteness
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:Degree
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :MorphosyntacticProperty .

:Determiner
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf lemon:Word ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :DeterminerPOS
    ] .

:DeterminerPOS
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :PartOfSpeech .

:DirectObject
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :Object .

:DitransitiveDoubleAccusativeFrame
    :example "das KOSTET ihn sein leben"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:cardinality "2"^^xsd:nonNegativeInteger ;
                owl:onProperty :directObject
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :subject
            ]
        )
    ] .

:DitransitiveFrame
    :example "i GAVE him it"@en, "mein vater SCHREIBT ihr einen brief"@de ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :directObject
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :indirectObject
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :subject
            ]
        )
    ] .

:DitransitiveFrame_For
    :example """she BOUGHT him it
she BOUGHT it FOR him"""@en ;
    :languageSpecific "en" ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
    rdfs:subClassOf :DitransitiveFrame .

:DitransitiveFrame_To
    :example """he GAVE his mother a present
he GAVE a present TO his mother"""@en ;
    :languageSpecific "en" ;
    a owl:Class ;
    rdfs:isDefinedBy <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
