            [
                a owl:Restriction ;
                owl:minCardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :object
            ]
            [
                a owl:Restriction ;
                owl:cardinality "0"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveImpersonalFrame> .

:AdjectivePOS
    a owl:Class ;
    rdfs:label "adjective"@en ;
    rdfs:subClassOf :PartOfSpeech ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectivePOS> .

:AdjectivePPFrame
    :example "he is RELATED TO her"@en ;
    a owl:Class ;
    rdfs:label "adjective pp frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePredicativeFrame
            :PrepositionalFrame
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectivePPFrame> .

:AdjectivePhrase
    a owl:Class ;
    rdfs:label "adjective phrase"@en ;
    rdfs:subClassOf <http://www.w3.org/ns/lemon/ontolex#MultiWordExpression> ;
    owl:equivalentClass [
        owl:intersectionOf (<http://www.w3.org/ns/lemon/ontolex#MultiWordExpression>
            [
                a owl:Restriction ;
                owl:onProperty :head ;
                owl:someValuesFrom :Adjective
            ]
        )
    ] .

:AdjectivePostPositiveFrame
    :example "that is something INTERESTING"@en ;
    a owl:Class ;
    rdfs:label "adjective post positive frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :copulativeArg
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :postPositiveArg
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectivePostPositiveFrame> .

:AdjectivePredicateFrame
    a owl:Class ;
    rdfs:comment "Note this frame should be used when both attributive and predicative use of the adjective is allowed"@en ;
    rdfs:label "adjective predicate frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveAttributiveFrame
            :AdjectivePredicativeFrame
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectivePredicateFrame> .

:AdjectivePredicativeFrame
    :example "he is HAPPY"@en ;
    a owl:Class ;
    rdfs:label "adjective predicative frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
