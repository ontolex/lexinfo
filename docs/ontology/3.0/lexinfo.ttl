:Adjunct
    rdfs:label "adjunct"@en ;
    rdfs:subClassOf <http://www.w3.org/ns/lemon/synsem#SyntacticArgument>, [
        a owl:Restriction ;
        owl:hasValue true ;
        owl:onProperty <http://www.w3.org/ns/lemon/synsem#optional>
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#Adjunct> .

