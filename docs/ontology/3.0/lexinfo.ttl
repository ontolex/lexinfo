            [
                a owl:Restriction ;
                owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveScaleFrame> .

:AdjectiveSuperlativeFrame
