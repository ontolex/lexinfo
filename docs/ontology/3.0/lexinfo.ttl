    vann:preferredNamespaceUri "http://www.lexinfo.net/ontology/3.0/lexinfo#"^^<http://www.w3.org/2001/XMLSchema#anyURI> ;
    a owl:Ontology ;
    rdfs:comment "An ontology of types, values and properties to be used with the OntoLex-Lemon model"@en ;
    rdfs:label "LexInfo"@en ;
    owl:imports <http://www.w3.org/ns/lemon/decomp>, <http://www.w3.org/ns/lemon/lime>, <http://www.w3.org/ns/lemon/ontolex>, <http://www.w3.org/ns/lemon/synsem>, <http://www.w3.org/ns/lemon/vartrans> ;
    owl:versionInfo "3.0" .

:AbbreviatedForm
    a owl:Class ;
    rdfs:subClassOf :TermType .

:AccusativePostPositiveArg
    rdfs:subClassOf :PostPositiveArg ;
    owl:equivalentClass [
        owl:intersectionOf (:PostPositiveArg
            [
                a owl:Restriction ;
                owl:hasValue :accusativeCase ;
                owl:onProperty <http://www.w3.org/ns/lemon/synsem#marker>
            ]
        )
    ] .

:AdjectivalComplementFrame
    a owl:Class ;
    rdfs:label "adjectival complement frame"@en ;
    rdfs:subClassOf :VerbFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:VerbFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :predicativeAdjective
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectivalComplementFrame> .

:Adjective
    a owl:Class ;
    rdfs:label "adjective"@en ;
    rdfs:subClassOf <http://www.w3.org/ns/lemon/ontolex#Word> ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty :partOfSpeech ;
        owl:someValuesFrom :AdjectivePOS
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#Adjective> .

:Adjective-i
    a owl:Class ;
    rdfs:label "adjective-i"@en ;
    rdfs:subClassOf :Adjective ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:hasValue :adjective-i ;
        owl:onProperty :partOfSpeech
    ] .

:Adjective-na
    a owl:Class ;
    rdfs:label "adjective-na"@en ;
    rdfs:subClassOf :Adjective ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:hasValue :adjective-na ;
        owl:onProperty :partOfSpeech
    ] .

:AdjectiveAccusativePostPositiveFrame
    :example "wird wurden unseren irrtum GEWAHR"@de ;
    a owl:Class ;
    rdfs:label "adjective accusative post positive frame"@en ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :AccusativePostPositiveArg
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveAccusativePostPositiveFrame> .

:AdjectiveAttributiveFrame
    :example "the RED ball"@en ;
    a owl:Class ;
    rdfs:label "adjective attributive frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:minCardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :attributiveArg
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveAttributiveFrame> .

:AdjectiveComparativeFrame
    :example "new york is BIGGER THAN berlin"@en ;
    a owl:Class ;
    rdfs:label "adjective comparative frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectiveFrame
            [
                a owl:Restriction ;
                owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :comparativeAdjunct
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
                owl:onProperty :copulativeSubject
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveComparativeFrame> .

:AdjectiveDativePostPositiveFrame
    :example "er ist seinem bruder Ã„HNLICH"@de ;
    a owl:Class ;
    rdfs:label "adjective dative post positive frame"@en ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :DativePostPositiveArg
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveDativePostPositiveFrame> .

:AdjectiveFrame
    a owl:Class ;
    rdfs:label "adjective frame"@en ;
    rdfs:subClassOf <http://www.w3.org/ns/lemon/synsem#SyntacticFrame> ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveFrame> .

:AdjectiveGenitivePostPositiveFrame
    :example "er ist des hochverrats SCHULDIG"@de ;
    a owl:Class ;
    rdfs:label "adjective genitive post positive frame"@en ;
    rdfs:subClassOf :AdjectivePostPositiveFrame ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (:AdjectivePostPositiveFrame
            [
                a owl:Restriction ;
                owl:onProperty :postPositiveArg ;
                owl:someValuesFrom :GenitivePostPositiveArg
            ]
        )
    ] ;
    owl:priorVersion <http://www.lexinfo.net/ontology/2.0/lexinfo#AdjectiveGenitivePostPositiveFrame> .

:AdjectiveImpersonalFrame
    :example "es ist mir HEISS"@de ;
    a owl:Class ;
    rdfs:label "adjective impersonal frame"@en ;
    rdfs:subClassOf :AdjectiveFrame ;
    owl:equivalentClass [
